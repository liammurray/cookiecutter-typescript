{
  "name": "{{ cookiecutter.repo_name }}",
  "version": "0.1.0",
  "description": "{{ cookiecutter.repo_description }}",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "audit-checker": "check-audit",
    "audit-resolver": "resolve-audit",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "lambda": "rimraf lambda && (cp -r dist lambda && cp package*.json ./lambda) && (cd ./lambda && npm ci --production)",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "test": "mocha test",
    "test:cov": "nyc npm test",
    "lint": "npm run lint:mdlint && npm run lint:eslint",
    "lint:mdlint": "markdownlint '*.md'",
    "lint:eslint": "eslint --cache --ext .ts ./",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "markdownlint": "^0.17.2",
    "markdownlint-cli": "^0.22.0",
    "mocha": "^6.2.2",
    "npm-audit-resolver": "^2.1.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "na"
  },
  "author": "{{ cookiecutter.repo_author }}",
  "license": "",
  "readme": "./README.md"
}
